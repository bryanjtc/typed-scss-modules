{
  "name": "typed-scss-modules",
  "version": "0.0.0",
  "description": "TypeScript type definition generator for SCSS CSS Modules",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "author": "Spencer Miskoviak <smiskoviak@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/skovy/typed-scss-modules.git"
  },
  "homepage": "https://github.com/skovy/typed-scss-modules.git#readme",
  "keywords": [
    "scss",
    "css modules",
    "cli",
    "typescript",
    "type generator",
    "scss modules"
  ],
  "scripts": {
    "test": "node --experimental-vm-modules $(yarn bin jest)",
    "test:watch": "node --experimental-vm-modules $(yarn bin jest --watch)",
    "typed-scss-modules": "tsx ./lib/cli.ts",
    "clean": "rm -rf ./dist",
    "build": "yarn clean && yarn tsup && chmod +x dist/cli.js",
    "prepare": "yarn build && husky install",
    "check-types": "tsc --noEmit",
    "check-formatting": "prettier --check '**/*.{js,json,css,md,scss,tsx,ts}'",
    "commit": "commit"
  },
  "files": [
    "dist"
  ],
  "bin": "./dist/cli.js",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@commitlint/prompt-cli": "^17.4.2",
    "@commitlint/travis-cli": "^17.4.2",
    "@frida/reserved-words": "^1.0.0",
    "@swc/core": "^1.3.29",
    "@swc/jest": "^0.2.24",
    "@types/css-modules-loader-core": "^1.1.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/node-sass": "^4.11.3",
    "@types/prettier": "^2.7.2",
    "@types/reserved-words": "^0.1.0",
    "@types/sass": "^1.43.1",
    "@types/yargs": "^17.0.20",
    "change-case": "^4.1.2",
    "colorette": "^2.0.19",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "node-sass": "^8.0.0",
    "node-sass-json-importer": "^4.3.0",
    "prettier": "^2.8.3",
    "sass": "^1.57.1",
    "semantic-release": "^20.1.0",
    "slash": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4",
    "yargs": "^17.6.2"
  },
  "peerDependencies": {
    "node-sass": "^7.0.3",
    "sass": "^1.49.7"
  },
  "peerDependenciesMeta": {
    "node-sass": {
      "optional": true
    },
    "sass": {
      "optional": true
    }
  },
  "dependencies": {
    "bundle-require": "^3.1.2",
    "chokidar": "^3.5.3",
    "css-modules-loader-core": "^1.1.0",
    "esbuild": "^0.16.17",
    "fast-glob": "^3.2.12",
    "joycon": "^3.1.1"
  },
  "lint-staged": {
    "*.{js,json,css,md,scss,tsx,ts}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@3.3.1"
}
